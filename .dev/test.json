{
    "debug": true,
    "transportLibrary": { "debug": true },
    "logging": {
        "appenders": {
            "consoleAppender": { "type": "stdout", "layout": { "type": "pattern", "pattern": "%[[%d] [%p] %c - %m %x{correlationId}%]" } },
            "console": { "type": "logLevelFilter", "level": "WARN", "appender": "consoleAppender" }
        },

        "categories": { "default": { "appenders": ["console"], "level": "TRACE" } }
    },
    "infrastructure": { "httpServer": { "enabled": true } },
    "modules": {
        "coreHttpApi": { "docs": { "enabled": true } },
        "webhooks": { "enabled": false, "webhooks": [{ "triggers": ["**"], "target": "test" }], "targets": { "test": {} } }
    }
}
